<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gx.springboot_test.mapper.UserGroupMapper">
    <!--用户组mapper部分-->
    <resultMap id="BaseResultMap" type="com.gx.springboot_test.po.UserGroup">
        <id column="user_group_id" jdbcType="INTEGER" property="userGroupId"/>
        <result column="user_group_name" jdbcType="VARCHAR" property="userGroupName"/>
        <result column="remark" jdbcType="VARCHAR" property="remark"/>
    </resultMap>
    <select id="selectAllUserGroup" resultMap="BaseResultMap">
        SELECT * FROM user_group
    </select>

</mapper>
